{
  "name": "tStory",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "export NODE_ENV=dev || SET \"NODE_ENV=dev\" && nodemon start",
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha --recursive",
    "test-watch": "nodemon --exec 'npm test'",
    "security-check": "nsp check",
    "lint": "esw app bin public routes views test repos config models",
    "lint-watch": "npm run lint -- --watch",
    "seed": "node ./config/seed"
  },
  "engines": {
    "node": "v6.9.3"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "mongoose": "^4.11.7",
    "mongoose-unique-validator": "^1.0.5",
    "morgan": "~1.8.1",
    "nodemon": "^1.11.0",
    "pug": "~2.0.0-beta11",
    "serve-favicon": "~2.4.2",
    "validator": "^8.0.0"
  },
  "devDependencies": {
    "nsp": "^2.7.0",
    "chai": "^4.0.2",
    "expect": "^1.20.2",
    "supertest": "^3.0.0",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "eslint": "^4.4.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-watch": "^3.1.2"
  }
}
